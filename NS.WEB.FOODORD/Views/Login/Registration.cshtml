@model NS.WEB.MODEL.UserModel
<form class="Form" method="post" enctype="multipart/form-data" asp-action="AddDish" asp-controller="Admin">
       <div asp-validation-summary="All" class="text-danger"></div>

      <div class="form-group">
        <label asp-for="CustomerName"></label>
        <input asp-for="CustomerName" class="form-control" />
        <span asp-validation-for="CustomerName" class="text-danger"></span>
      </div>

    <div class="form-group">
       <label asp-for="CustomerEmail"></label>
       <textarea asp-for="CustomerEmail" class="form-control"></textarea>
       <span asp-validation-for="CustomerEmail" class="text-danger"></span>
    </div>

    <div class="form-group">
       <label asp-for="CustomerAddress"></label>
       <textarea asp-for="CustomerAddress" class="form-control"></textarea>
       <span asp-validation-for="CustomerAddress" class="text-danger"></span>
    </div>

    <div class="form-group">
       <label asp-for="CityName"></label>
       <textarea asp-for="CityName" class="form-control"></textarea>
       <span asp-validation-for="CityName" class="text-danger"></span>
    </div>

    <div class="form-group">
       <label asp-for="State"></label>
       <textarea asp-for="State" class="form-control"></textarea>
       <span asp-validation-for="State" class="text-danger"></span>
    </div>
    
    <div class="form-group">
       <label asp-for="Password"></label>
       <textarea asp-for="Password" class="form-control" typeof="password"></textarea>
       <span asp-validation-for="Password" class="text-danger"></span>
    </div>
    <div class="form-group">
       <label asp-for="ConfirmPassword"></label>
       <textarea asp-for="ConfirmPassword" class="form-control" typeof="password"></textarea>
       <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
    </div>
  @* <div class="form-group">
       <label asp-for="DishCategory"></label>
       <br />
        <span>VEGITARIAN: </span> @Html.RadioButton("DishCategory","VEGITARIAN",true)
        <span> | NON-VEGITARIAN: </span> @Html.RadioButton("DishCategory", "NON-VEGITARIAN",false)
   </div>*@

    <div class="form-group">
       <label asp-for="ZipCode"></label>
       <input asp-for="ZipCode" class="form-control" />
       <span asp-validation-for="ZipCode" class="text-danger"></span>
    </div>
      
    

    <div class="form-group">
       <label asp-for="UserPhoto"></label>
      <div class="custom-file">
         
          @Html.TextBoxFor(m => m.UserPhoto, new { type = "file", multiple = "multiple"})
      </div>
       <span asp-validation-for="UserPhoto" class="text-danger"></span>
    </div>


     <div class="form-group">
        <input type="submit" value="AddUser" class="btn btn-primary" />
    </div>

   </form>